{% extends 'base.html.twig' %}

{% block body %}
    <h1>Support_Information list</h1>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Description</th>
                <th>Value</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for support_Information in support_Informations %}
            <tr>
                <td><a href="{{ path('admin_support_information_show', { 'id': support_Information.id }) }}">{{ support_Information.id }}</a></td>
                <td>{{ support_Information.name }}</td>
                <td>{{ support_Information.description }}</td>
                <td>{{ support_Information.value }}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('admin_support_information_show', { 'id': support_Information.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('admin_support_information_edit', { 'id': support_Information.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('admin_support_information_new') }}">Create a new entry</a>
        </li>
    </ul>
{% endblock %}
