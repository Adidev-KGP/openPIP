{% extends 'base.html.twig' %}

{% block body %}
    <h1>Master_Table</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ master_Table.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ master_Table.name }}</td>
            </tr>
            <tr>
                <th>Source</th>
                <td>{{ master_Table.source }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('admin_master_table_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('admin_master_table_edit', { 'id': master_Table.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
