                						<div class="col-sm-7">
                					        {{ form_label(form.verified_edge_color) }} 
                           			    </div>
                         			    <div class="col-sm-3">                  					        
                					        {{ form_widget(form.verified_edge_color) }}
                					    </div>
                					</div>    
