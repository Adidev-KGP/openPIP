                						<div class="col-sm-7">
                					        {{ form_label(form.literature_edge_color) }} 
                           			    </div>
                         			    <div class="col-sm-3">   		        
                					        
                					        {{ form_widget(form.literature_edge_color) }}
                						   </div>
        							</div>   						
    							</div>
    						</div>
						</div>
					</div>
    			</div>
